Candidates Table
<table style="width:100%">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Note</th>
    <th>Created at</th>
  </tr>
  <% @candidates.each do |candidate| %>
    <tr>
      <td><%= candidate.full_name %></td>
      <td><%= candidate.email %></td>
      <td><%= candidate.phone %></td>
      <td><%= candidate.note %></td>
      <td><%= candidate.created_at %></td>
    </tr>
  <% end %>
</table>